{
    "manifest_version": 2,
    "name": "LeetCode Hide",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "https://leetcode.com/problems/*"
            ],
            "js": [
                "jquery-3.4.1.min.js", "content.js"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": ["background.js"]
    }
}